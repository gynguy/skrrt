local LIB = loadstring(game:HttpGet("https://raw.githubusercontent.com/gynguy/skrrt/master/uilib",true))()
local on = false
local FeHub = LIB:CreateSection("FE")

FeHub:Button("Ctrl TP", function(variable)
  if variable then
    on = true
  else
    on = false
  end
   while on do
 loadstring(game:HttpGet("https://raw.githubusercontent.com/gynguy/skrrt/master/clicktp",true))()
  end
end)

